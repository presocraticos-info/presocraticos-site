@article{Trindade2011,
	abstract = {Is it possible to assert that there is no such thing as false knowledge without implying that all knowledge is necessarily infallible? If \&ldquo;what is not\&rdquo; cannot be known, knowledge must be of \&ldquo;what is\&rdquo;. This conclusion is condensed in the thesis according to which \&ldquo;thinking (knowing) and being are the same\&rdquo; {(B3}, B8.34). Reading einai existentially, one interpretation extracts being from thinking, attributing to noein the power of generating reality (or asserting that \&ldquo;being thinks\&rdquo;). To avoid this consequence, another interpretation deprives einai of any reference to reality. But does it not deflate the identity thesis into an innocuous tautology? This dilemma may be eschewed if \&lsquo;thinking\&rsquo; and \&lsquo;being\&rsquo; are reformulated in a way that affects their mutual relation as well as \&ldquo;the opinions of mortals\&rdquo;.},
	author = {José Gabriel Trindade Santos},
	date-added = {2024-05-26 14:46:46 -0300},
	date-modified = {2024-05-26 19:02:03 -0300},
	issn = {2177-5346},
