	owner = {brunoc},
	timestamp = {2012.02.01},
	title = {Pensando o ser, no Poema de Parmênides},
	url = {https://www.hypnos.org.br/index.php/hypnos/article/view/224},
	volume = {26},
	year = {2011}}

